{
	"schemaVersion": 1,
	"id": "elytratrails",
	"version": "${version}",
	"name": "Elytra Contrails",
	"accessWidener" : "elytratrails.classtweaker",
	"description": "Pretty trails when you are flying!",
	"authors": [
		"dbrighthd"
	],
	"contact": {
		"sources": "https://github.com/dbrighthd/elytratrails",
		"issues" : "https://github.com/dbrighthd/elytratrails/issues"
	},
	"license": "MPL-2.0",
	"icon": "assets/elytratrails/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"dbrighthd.elytratrails.ElytraTrails"
		],
		"client": [
			"dbrighthd.elytratrails.ElytraTrailsClient"
		],
		"modmenu": ["dbrighthd.elytratrails.ElytraTrailsModMenu"]
	},
	"mixins": [
		"elytratrails.mixins.json",
